{% extends 'base.html.twig' %} {% block title %}Level index{% endblock %} {%
block body %}
<div class="container">
  <h1 class="worspace">les classes</h1>

  <table class="table">
    <thead>
      <tr>
        <th>Id</th>
        <th>Name</th>
        <th>actions</th>
      </tr>
    </thead>
    <tbody>
      {% for level in levels %}
      <tr>
        <td>{{ level.id }}</td>
        <td>{{ level.name }}</td>
        <td>
          <a
            class="btn btn-secondary"
            href="{{ path('app_level_show', { id: level.id }) }}"
            >show</a
          >
          <a
            class="btn btn-success"
            href="{{ path('app_level_edit', { id: level.id }) }}"
            >edit</a
          >
        </td>
      </tr>
      {% else %}
      <tr>
        <td colspan="3">no records found</td>
      </tr>
      {% endfor %}
    </tbody>
  </table>

  <a class="btn btn-primary" href="{{ path('app_level_new') }}">Create new</a>
</div>

{% endblock %}
